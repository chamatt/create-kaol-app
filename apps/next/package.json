    "next": "12.3.2",
    "raf": "^3.4.1"
  },
  "devDependencies": {
    "@types/cookies": "^0.7.7",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "17.0.21",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^9.0.0",
    "npm-run-all": "^4.1.5"
  }
